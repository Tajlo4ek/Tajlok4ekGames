<Window x:Class="DialogForm.YesNoDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DialogForm"
        mc:Ignorable="d"
        Title="YesNoDialog" Padding="10" Width="288" Height="135.667" WindowStartupLocation="CenterOwner" ResizeMode="NoResize">
    <Grid Margin="10,10,10,8" RenderTransformOrigin="0.56,0.16" Width="260">
        <Label x:Name="label" Content="asd" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="235"/>
        <Button x:Name="btnCancel" Content="Отмена" HorizontalAlignment="Left" Margin="10,57,0,0" VerticalAlignment="Top" Width="75" Click="BtnCancel_Click"/>
        <Button x:Name="btnNo" Content="Нет" HorizontalAlignment="Left" Margin="90,57,0,0" VerticalAlignment="Top" Width="75" Click="BtnNo_Click"/>
        <Button x:Name="btnYes" Content="Да" HorizontalAlignment="Left" Margin="170,57,0,0" VerticalAlignment="Top" Width="75" Click="BtnYes_Click"/>
    </Grid>
</Window>
